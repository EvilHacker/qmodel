{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Start Dev Server",
			"group": "build",
			"type": "npm",
			"script": "start",
			"isBackground": true,
			"problemMatcher": [
				{
					"pattern": "$eslint-stylish",
					"background": {
						"activeOnStart": true,
						"beginsPattern": "^rollup v[0-9.]+$",
						"endsPattern": "] waiting for changes...$"
					}
				}
			]
		},
		{
			"label": "Start Prod Server",
			"group": "build",
			"type": "npm",
			"script": "start-prod",
			"isBackground": true,
			"problemMatcher": [
				{
					"pattern": "$eslint-stylish",
					"background": {
						"activeOnStart": true,
						"beginsPattern": "^rollup v[0-9.]+$",
						"endsPattern": "] waiting for changes...$"
					}
				}
			]
		},
		{
			"label": "Build",
			"group": "build",
			"type": "npm",
			"script": "build",
			"problemMatcher": [
				"$eslint-stylish"
			]
		},
		{
			"label": "Build Prod",
			"group": "build",
			"type": "npm",
			"script": "build-prod",
			"problemMatcher": [
				"$eslint-stylish"
			]
		},
		{
			"label": "Build Dev",
			"group": "build",
			"type": "npm",
			"script": "build-dev",
			"problemMatcher": [
				"$eslint-stylish"
			]
		},
		{
			"label": "Clean",
			"group": "build",
			"type": "npm",
			"script": "clean",
			"problemMatcher": []
		},
		{
			"label": "Clean All",
			"group": "build",
			"type": "npm",
			"script": "clean-all",
			"problemMatcher": []
		},
	]
}